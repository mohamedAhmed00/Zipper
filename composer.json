{
    "name": "mohamedahmed00/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "https://github.com/mohamedAhmed00/Zipper",
    "license": "Apache2",
    "authors": [
        {
            "name": "Nils Plaschke",
            "email": "github@nilsplaschke.de",
            "homepage": "http://nilsplaschke.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-zip": "*",
        "illuminate/support": "^11.23",
        "illuminate/filesystem": "^11.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "MohamedAhmed00\\Zipper\\": "src/mohamedAhmed00/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MohamedAhmed00\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "mohamedAhmed00\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "mohamedAhmed00\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
